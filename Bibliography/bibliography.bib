@article{springer.171CD9820170101,
Abstract = {3D cell culture is an invaluable tool in developmental, cell, and cancer biology. By mimicking crucial features of in vivo environment, including cell–cell and cell–extracellular matrix interactions, 3D cell culture enables proper structural architecture and differentiated function of normal tissues or tumors in vitro. Thereby 3D cell culture realistically models in vivo tissue conditions and processes, and provides in vivo like responses. Since its early days in the 1970s, 3D cell culture has revealed important insights into mechanisms of tissue homeostasis and cancer, and accelerated translational research in cancer biology and tissue engineering.},
Author = {Koledova, Zuzana, Editor and Koledova, Zuzana and Walker, John M., Series editor},
ISSN = {978-1-4939-7021-6},
Journal = {3D Cell Culture : Methods and Protocols},
