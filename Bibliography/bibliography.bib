@article{springer.21E62A9120110101,
Abstract = {The measurement of cell viability plays a fundamental role in all forms of cell culture. Sometimes it is the main purpose of the experiment, such as in toxicity assays. Alternatively, cell viability can be used to ­correlate cell behaviour to cell number, providing a more accurate picture of, for example, anabolic ­activity. There are wide arrays of cell viability methods which range from the most routine trypan blue dye exclusion assay to highly complex analysis of individual cells, such as using RAMAN microscopy. The cost, speed, and complexity of equipment required will all play a role in determining the assay used. This chapter aims to provide an overview of many of the assays available today.},
Author = {Stoddart, Martin J., Editor and Stoddart, Martin J.},
ISSN = {978-1-61779-108-6},
Journal = {Mammalian Cell Viability : Methods and Protocols},
Keywords = {Apoptosis, Viability, Multiplexing, 3D, 2D, Life Sciences, Cell Biology, Cell Culture, Biomedical and Life Sciences},
Pages = {1},
Title = {Cell Viability Assays: Introduction.},
URL = {http://lp.hscl.ufl.edu/login?url=https://search.ebscohost.com/login.aspx?direct=true&AuthType=ip,uid&db=edsspo&AN=springer.21E62A91&site=eds-live},
Year = {2011},
